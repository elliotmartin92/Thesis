  author = {Le Goff, Salom{\'e} and Boussaid, Ismael and Floquet, Celia and Raimbault, Anna and Hatin, Isabelle and {Andrieu-Soler}, Charlotte and Salma, Mohammad and Leduc, Marjorie and Gautier, Emilie-Fleur and Guyot, Boris and {d'Allard}, Diane and {Montel-Lehry}, Nathalie and Ducamp, Sarah and Houvert, Amandine and Guillonneau, Fran{\c c}ois and Giraudier, St{\'e}phane and {Cramer-Bord{\'e}}, Elisabeth and Morl{\'e}, Fran{\c c}ois and Diaz, Jean-Jacques and Hermine, Olivier and Taylor, Naomi and Kinet, Sandrina and Verdier, Fr{\'e}d{\'e}rique and Padua, Rose-Ann and Mohandas, Narla and Gleizes, Pierre-Emmanuel and Soler, Eric and Mayeux, Patrick and Fontenay, Michaela},
