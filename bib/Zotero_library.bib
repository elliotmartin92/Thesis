@article{rubinMixingMatchingChromosomes2020,
  title = {Mixing and {{Matching Chromosomes}} during {{Female Meiosis}}},
  author = {Rubin, Thomas and Macaisne, Nicolas and Huynh, Jean-Ren{\'e}},
