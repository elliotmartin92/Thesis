@article{Fu2014c,
  title = {Gene Expression Regulation Mediated through Reversible m 6 {{A RNA}} Methylation},
  shorttitle = {Fu2014c},
  author = {Fu, Ye and Dominissini, Dan and Rechavi, Gideon and He, Chuan},
  year = {2014},
