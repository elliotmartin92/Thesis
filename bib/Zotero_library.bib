@article{Khong2016a,
  title = {Temporal Regulation of Distinct Internal Ribosome Entry Sites of the Dicistroviridae Cricket Paralysis Virus},
  shorttitle = {Khong2016a},
  author = {Khong, Anthony and Bonderoff, Jennifer M. and Spriggs, Ruth V. and Tammpere, Erik and Kerr, Craig H. and Jackson, Thomas J. and Willis, Anne E. and Jan, Eric},
  year = {2016},
