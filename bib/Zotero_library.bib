  number = {4},
  pages = {1125--1135},
  issn = {0950-1991},
  abstract = {The hedgehog (hh) gene plays a role in regulating cell proliferation and specifying cell identity in diverse systems. We show that hh is expressed at the extreme apical end of Drosophila ovarioles in terminal filament cells and a newly identified group of associated somatic cells. Reducing or ectopically expressing hh affects somatic cells in region 2 of the germarium, 2-5 cells away from the cells in which Hh protein is detected. hh activity stimulates the proliferation of pre-follicle somatic cells, and promotes the specification of polar follicle cells. hh signaling during egg chamber assembly appears to be closely related to, or part of pathways involving the neurogenic genes.},
  langid = {english},
  pmid = {8620839},
  keywords = {Animals,Cell Division,Cell Polarity,Drosophila,Drosophila Proteins,Female,Gene Expression Regulation; Developmental,Genes; Insect,Hedgehog Proteins,Oocytes,Oogenesis,Ovary,Proteins,Stem Cells}
}

@article{fortierHaploinsufficiencyScreenHighlights2015,
  title = {Haploinsufficiency Screen Highlights Two Distinct Groups of Ribosomal Protein Genes Essential for Embryonic Stem Cell Fate},
  author = {Fortier, Simon and MacRae, Tara and Bilodeau, M{\'e}lanie and Sargeant, Tobias and Sauvageau, Guy},
  year = {2015},
