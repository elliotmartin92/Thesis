@article{koziolTpt1ActivatesTranscription2007,
  title = {Tpt1 {{Activates Transcription}} of Oct4 and Nanog in {{Transplanted Somatic Nuclei}}},
  author = {Koziol, Magdalena J. and Garrett, Nigel and Gurdon, J. B.},
