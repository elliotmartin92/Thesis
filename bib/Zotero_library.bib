@article{Chothani,
  title = {Reliable Detection of Translational Regulation with {{Ribo-seq}}},
  shorttitle = {Chothani},
  author = {Chothani, Sonia P and Adami\hspace{0pt} 1, Eleonora and Sivakumar and 1, Viswanathan and Orbert Hubner, N and Cook, Stuart and Schafer, Sebastian and Rackham, Owen J L},
  doi = {10.1101/234344},
  abstract = {Ribosome profiling\hspace{0pt} 1 (Ribo-Seq) reveals genome-wide translation rates via the quantification of ribosome protected fragments (RPFs) of mRNAs. Significant differences of RPFs between conditions can indicate differential translation if they do not match changes in transcription. Reliably identifying translational differences is complicated by the fact that the mRNA level of the transcript directly affects the ribosome occupancy. Changes in mRNA abundances result in changes in RPFs due to differential transcription and not translation. As such, identification of differentially translated genes (DTGs) requires finding significant changes in RPF read counts that cannot be explained by variation in mRNA read counts. Xiao et al.\hspace{0pt} 2 developed Xtail with the purpose of more accurately identifying DTGs in pairwise comparisons. Several other methods were also recently released (Ribodiff\hspace{0pt} 3 and Riborex\hspace{0pt} 4\hspace{0pt}). At their core, all of these approaches either utilize existing differential expression programs (eg DEseq2\hspace{0pt} 5 and EdgeR\hspace{0pt} 6\hspace{0pt}) or use similar statistical assumptions to model the data. However, none of these methods allow for complex experimental design (with more than two conditions) or the use of alternative statistical setups (such as the likelihood ratio tests for comparisons across time) and crucially they do not allow for correction of any batch effects, a commonality to almost all sequencing experiments. Although standalone tools for batch correcting sequencing data exist\hspace{0pt} 7\hspace{0pt} , differential expression tools for count data require raw read counts to accurately model sample-to-sample variation\hspace{0pt} 8\hspace{0pt} .}
}

