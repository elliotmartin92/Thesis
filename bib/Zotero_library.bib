@article{Kim-Ha1995i,
  title = {Translational Regulation of Oskar {{mRNA}} by {{Bruno}}, an Ovarian {{RNA-binding}} Protein, Is Essential},
  shorttitle = {Kim-{{Ha1995i}}},
  author = {{Kim-Ha}, Jeongsil and Kerr, Karen and Macdonald, Paul M},
  year = {1995},
