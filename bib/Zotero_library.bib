@article{Flora2018k,
  title = {Sequential {{Regulation}} of {{Maternal mRNAs}} through a {{Conserved}} Cis-{{Acting Element}} in {{Their}} 3' {{UTRs}}},
  shorttitle = {Flora2018k},
  author = {Flora, Pooja and {Wong-Deyrup}, Siu Wah and Martin, Elliot Todd and Palumbo, Ryan J and Nasrallah, Mohamad and Oligney, Andrew and Blatt, Patrick and Patel, Dhruv and Fuchs, Gabriele and Rangan, Prashanth},
  year = {2018},
