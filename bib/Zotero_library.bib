@article{petroccaE2F1RegulatedMicroRNAsImpair2008,
  title = {{{E2F1-Regulated MicroRNAs Impair TGF$\beta$-Dependent Cell-Cycle Arrest}} and {{Apoptosis}} in {{Gastric Cancer}}},
  author = {Petrocca, Fabio and Visone, Rosa and Onelli, Mariadele Rapazzotti and Shah, Manisha H. and Nicoloso, Milena S. and {de Martino}, Ivana and Iliopoulos, Dimitrios and Pilozzi, Emanuela and Liu, Chang-Gong and Negrini, Massimo and Cavazzini, Luigi and Volinia, Stefano and Alder, Hansjuerg and Ruco, Luigi P. and Baldassarre, Gustavo and Croce, Carlo M. and Vecchione, Andrea},
