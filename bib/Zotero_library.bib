@article{maDNADamageinducedLok2016,
  title = {{{DNA}} Damage-Induced {{Lok}}/{{CHK2}} Activation Compromises Germline Stem Cell Self-Renewal and Lineage Differentiation},
  author = {Ma, Xing and Han, Yingying and Song, Xiaoqing and Do, Trieu and Yang, Zhihao and Ni, Jianquan and Xie, Ting},
  year = {2016},
  month = dec,
  journal = {Development},
  volume = {143},
  number = {23},
  pages = {4312--4323},
  issn = {0950-1991, 1477-9129},
  doi = {10.1242/dev.141069},
  abstract = {Stem cells in adult tissues are constantly exposed to genotoxic stress and also accumulate DNA damage with age. However, it remains largely unknown how DNA damage affects both stem cell selfrenewal and differentiation. In this study, we show that DNA damage retards germline stem cell (GSC) self-renewal and progeny differentiation in a Lok kinase-dependent manner in the Drosophila ovary. Both heatshock-inducible endonuclease I-CreI expression and X-ray irradiation can efficiently introduce double-strand breaks in GSCs and their progeny, resulting in a rapid GSC loss and a GSC progeny differentiation defect. Surprisingly, the elimination of Lok or its kinase activity can almost fully rescue the GSC loss and the progeny differentiation defect caused by DNA damage induced by I-CreI or X-ray. In addition, the reduction in bone morphogenetic protein signaling and Shotgun expression only makes a limited contribution to DNA damage-induced GSC loss. Finally, DNA damage also decreases the expression of the master differentiation factor Bam in a Lok-dependent manner, which helps explain the GSC progeny differentiation defect. Therefore, this study demonstrates, for the first time in vivo, that Lok kinase activation is required for the DNA damage-mediated disruption of adult stem cell self-renewal and lineage differentiation, and might also offer novel insight into how DNA damage causes tissue aging and cancer formation.},
  langid = {english},
  file = {C\:\\Users\\Elliot\\Zotero\\storage\\NGRFAETL\\Ma et al. - 2016 - DNA damage-induced LokCHK2 activation compromises.pdf}
}

