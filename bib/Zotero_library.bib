  keywords = {Cytoplasm,Drosophila melanogaster,Eggs,Messenger RNA,Ovaries,Phenotypes,Protein translation,Ribosomes},
  file = {C\:\\Users\\Elliot\\Zotero\\storage\\YW2VMG54\\Minakhina et al. - 2016 - Zfrp8PDCD2 Interacts with RpS2 Connecting Ribosom.pdf;C\:\\Users\\Elliot\\Zotero\\storage\\MKRCNQ46\\article.html}
}

@article{mironSignalingAktFRAP2003,
