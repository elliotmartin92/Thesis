@article{ayyazSinglecellTranscriptomesRegenerating,
  title = {Single-Cell Transcriptomes of the Regenerating Intestine Reveal a Revival Stem Cell},
  author = {Ayyaz, Arshad and Kumar, Sandeep and Sangiorgi, Bruno and Ghoshal, Bibaswan and Gosio, Jessica and Ouladan, Shaida and Fink, Mardi and Barutcu, Seda and Trcka, Daniel and Shen, Jess and Chan, Kin and Wrana, Jeffrey L. and Gregorieff, Alex},
  journal = {Nature},
  doi = {10.1038/s41586-019-1154-y},
  abstract = {The turnover of the intestinal epithelium is driven by multipotent LGR5\textsuperscript{+} crypt-base columnar cells (CBCs) located at the bottom of crypt zones\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR1"{$>$}1{$<$}/a{$>$}}. However, CBCs are lost following injury, such as irradiation\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR2"{$>$}2{$<$}/a{$>$}}, but the intestinal epithelium is nevertheless able to recover\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR3"{$>$}3{$<$}/a{$>$}}. Thus, a second population of quiescent '+4' cells, or reserve stem cells (RSCs), has~previously been proposed to regenerate the damaged intestine\textsuperscript{{$<$}a href="\#ref-CR4"{$>$}4{$<$}/a{$>$},{$<$}a href="\#ref-CR5"{$>$}5{$<$}/a{$>$},{$<$}a href="\#ref-CR6"{$>$}6{$<$}/a{$>$},{$<$}a href="/articles/s41586-019-1154-y\#ref-CR7"{$>$}7{$<$}/a{$>$}}. Although CBCs and RSCs were thought to be mutually exclusive\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR4"{$>$}4{$<$}/a{$>$},{$<$}a href="/articles/s41586-019-1154-y\#ref-CR8"{$>$}8{$<$}/a{$>$}}, subsequent studies~have found that LGR5\textsuperscript{+} CBCs express RSC markers\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR9"{$>$}9{$<$}/a{$>$}} and that RSCs were dispensable\textemdash whereas LGR5\textsuperscript{+} cells were essential\textemdash for repair of the damaged intestine\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR3"{$>$}3{$<$}/a{$>$}}. In addition, progenitors of absorptive enterocytes\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR10"{$>$}10{$<$}/a{$>$}}, secretory cells\textsuperscript{{$<$}a href="\#ref-CR11"{$>$}11{$<$}/a{$>$},{$<$}a href="\#ref-CR12"{$>$}12{$<$}/a{$>$},{$<$}a href="\#ref-CR13"{$>$}13{$<$}/a{$>$},{$<$}a href="\#ref-CR14"{$>$}14{$<$}/a{$>$},{$<$}a href="/articles/s41586-019-1154-y\#ref-CR15"{$>$}15{$<$}/a{$>$}}~and slow cycling LGR5\textsuperscript{+} cells\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR16"{$>$}16{$<$}/a{$>$}} have been shown to contribute to regeneration whereas the transcriptional regulator YAP1, which is important for intestinal regeneration, was suggested to induce a pro-survival phenotype in LGR5\textsuperscript{+} cells\textsuperscript{{$<$}a href="/articles/s41586-019-1154-y\#ref-CR17"{$>$}17{$<$}/a{$>$}}. Thus, whether cellular plasticity or distinct cell populations are critical for intestinal regeneration remains unknown. Here we applied single-cell RNA sequencing to profile the regenerating mouse intestine and identified a distinct, damage-induced quiescent cell type that we term the revival stem cell (revSC). revSCs are marked by high clusterin expression and are extremely rare under homoeostatic conditions, yet give rise\textemdash in a temporal hierarchy\textemdash to all the major cell types of the intestine, including LGR5\textsuperscript{+} CBCs. After intestinal damage by irradiation, targeted ablation of LGR5\textsuperscript{+} CBCs, or treatment with dextran sodium sulfate, revSCs undergo a YAP1-dependent transient expansion, reconstitute the LGR5\textsuperscript{+} CBC compartment and are required to regenerate a functional intestine. These studies thus define a unique stem cell that is mobilized by damage to revive the homoeostatic stem cell compartment and regenerate the intestinal epithelium.}
}

@article{bagnaraVitroGrowthRegulation1991,
  title = {In {{Vitro Growth}} and {{Regulation}} of {{Bone Marrow Enriched CD34}}+ {{Hematopoietic Progenitors}} in {{Diamond-Blackfan Anemia}}},
  author = {Bagnara, G. P. and Zauli, G. and Vitale, L. and Rosito, P. and Vecchi, V. and Paolucci, G. and Avanzi, G. C. and Ramenghi, U. and Timeus, F. and Gabutti, V.},
  year = {1991},
  month = nov,
  journal = {Blood},
  volume = {78},
  number = {9},
  pages = {2203--2210},
  issn = {0006-4971},
  doi = {10.1182/blood.V78.9.2203.2203},
  abstract = {Diamond-Blackfan anemia (DBA) is a congenital red blood cell aplasia. No clear explanation has been given of its defective erythropoiesis, although different humoral or cellular inhibitory factors have been proposed. To clarify the nature of this defect we studied the effect of several human recombinant growth factors on an enriched CD34+ population obtained from the bone marrow of 10 DBA patients. We observed a defect underlying the early erythroid progenitors, which were unresponsive to several growth factors (erythro-poietin, interleukin-3 [IL-3], IL-6, granulocyte-macrophage colony-stimulating factor [GM-CSF], erythroid potentiating activity), either alone or in association. The production of cytokines was not impaired, and high levels of IL-3 and GM-CSF were found in phytohemagglutinin-leukocyte-conditioned medium (PHA-LCM) when tested with a sensitive biologic assay on the M-07E cell line. Hematopoietic stem cells in DBA patients may be induced to differentiate to the granulocyte megakaryocyte, but not the erythroid compartment, as shown after CD34+ cell preincubation with IL-3. Addition of the stem cell factor to IL-3 and erythropoietin induces a dramatic in vitro increase in both the number and the size of BFU-E, which also display a normal morphologic terminal differentiation.},
  langid = {english},
  file = {C\:\\Users\\Elliot\\Zotero\\storage\\35JU2YG2\\Bagnara et al. - 1991 - In Vitro Growth and Regulation of Bone Marrow Enri.pdf;C\:\\Users\\Elliot\\Zotero\\storage\\WPERJWGI\\S0006497120821867.html}
}

