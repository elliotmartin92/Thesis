@article{martinPreribosomalRNAInteraction2014,
  title = {A Pre-Ribosomal {{RNA}} Interaction Network Involving {{snoRNAs}} and the {{Rok1}} Helicase},
  author = {Martin, Roman and Hackert, Philipp and Ruprecht, Maike and Simm, Stefan and Br{\"u}ning, Lukas and Mirus, Oliver and Sloan, Katherine E. and Kudla, Grzegorz and Schleiff, Enrico and Bohnsack, Markus T.},
  year = {2014},
  month = aug,
  journal = {RNA},
  volume = {20},
