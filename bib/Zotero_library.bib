@article{chenComprehensiveAnalysisNucleocytoplasmic2017,
  title = {Comprehensive Analysis of Nucleocytoplasmic Dynamics of {{mRNA}} in {{Drosophila}} Cells},
  author = {Chen, Tao and van Steensel, Bas},
  year = {2017},
  month = aug,
  journal = {PLOS Genetics},
  volume = {13},
  number = {8},
  pages = {e1006929},
  publisher = {{Public Library of Science}},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1006929},
  abstract = {Eukaryotic mRNAs undergo a cycle of transcription, nuclear export, and degradation. A major challenge is to obtain a global, quantitative view of these processes. Here we measured the genome-wide nucleocytoplasmic dynamics of mRNA in Drosophila cells by metabolic labeling in combination with cellular fractionation. By mathematical modeling of these data we determined rates of transcription, export and cytoplasmic decay for 5420 genes. We characterized these kinetic rates and investigated links with mRNA features, RNA-binding proteins (RBPs) and chromatin states. We found prominent correlations between mRNA decay rate and transcript size, while nuclear export rates are linked to the size of the 3'UTR. Transcription, export and decay rates are each associated with distinct spectra of RBPs. Specific classes of genes, such as those encoding cytoplasmic ribosomal proteins, exhibit characteristic combinations of rate constants, suggesting modular control. Binding of splicing factors is associated with faster rates of export, and our data suggest coordinated regulation of nuclear export of specific functional classes of genes. Finally, correlations between rate constants suggest global coordination between the three processes. Our approach provides insights into the genome-wide nucleocytoplasmic kinetics of mRNA and should be generally applicable to other cell systems.},
  langid = {english},
  keywords = {3' UTR,Chromatin,DNA transcription,Drosophila melanogaster,Introns,Messenger RNA,RNA sequencing,RNA-binding proteins},
  file = {C\:\\Users\\Elliot\\Zotero\\storage\\S94FB92E\\Chen and Steensel - 2017 - Comprehensive analysis of nucleocytoplasmic dynami.pdf;C\:\\Users\\Elliot\\Zotero\\storage\\JRGYGU9L\\article.html}
}

