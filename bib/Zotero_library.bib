@article{fuchsProteomicAnalysisRibosomes2011,
  title = {Proteomic {{Analysis}} of {{Ribosomes}}: {{Translational Control}} of {{mRNA}} Populations by {{Glycogen Synthase GYS1}}},
  shorttitle = {Proteomic {{Analysis}} of {{Ribosomes}}},
  author = {Fuchs, Gabriele and Diges, Camille and Kohlstaedt, Lori A. and Wehner, Karen A. and Sarnow, Peter},
  year = {2011},
  month = jul,
  journal = {Journal of molecular biology},
  volume = {410},
  number = {1},
  pages = {118--130},
  publisher = {{Academic Press}},
  issn = {0022-2836},
  doi = {10.1016/j.jmb.2011.04.064},
  abstract = {Ribosomes exist as a heterogenous pool of macromolecular complexes composed of ribosomal RNA molecules, ribosomal proteins and numerous associated, ``non-ribosomal'' proteins. To identify non-ribosomal proteins that may modulate ribosome activity, we examined the composition of translationally active and inactive ribosomes using a proteomic multidimensional protein identification technology. Notably, the phosphorylated isoform of glycogen synthase, GYS1, was preferentially associated with elongating ribosomes. Depletion of GYS1 affected the translation of a subset of cellular mRNAs, some of which encode proteins that modulate protein biosynthesis. These findings argue that GYS1 abundance, and by virtue of its ribosomal association, provide a feedback loop between the energy state of the cells and the translation machinery.},
  pmcid = {PMC3131224},
  pmid = {21570405},
  file = {C\:\\Users\\Elliot\\Zotero\\storage\\AQZA9NT8\\Fuchs et al. - 2011 - Proteomic Analysis of Ribosomes Translational Control of mRNA Populations by Glycogen Synthase GYS1(2).pdf;C\:\\Users\\Elliot\\Zotero\\storage\\E8CBLSIJ\\Fuchs et al. - 2011 - Proteomic Analysis of Ribosomes Translational Con.pdf;C\:\\Users\\Elliot\\Zotero\\storage\\R2CAJ89G\\Fuchs et al. - 2011 - Proteomic Analysis of Ribosomes Translational Con.pdf;C\:\\Users\\Elliot\\Zotero\\storage\\V2IW2IX4\\S0022283611005079.html}
}

@article{Fukao2009c,
  title = {The {{ELAV Protein HuD Stimulates Cap-Dependent Translation}} in a {{Poly}}({{A}})- and {{eIF4A-Dependent Manner}}},
  shorttitle = {Fukao2009c},
  author = {Fukao, Akira and Sasano, Yumi and Imataka, Hiroaki and Inoue, Kunio and Sakamoto, Hiroshi and Sonenberg, Nahum and Thoma, Christian and Fujiwara, Toshinobu},
  year = {2009},
  journal = {Molecular Cell},
  volume = {36},
  number = {6},
  pages = {1007--1017},
  issn = {1097-2765},
  doi = {10.1016/j.molcel.2009.11.013},
  abstract = {Summary The RNA-binding protein HuD promotes neuronal differentiation by an unknown mechanism. Here we~identify an enhancer function of HuD in translation. Translation stimulation by HuD requires both a 3{${'}$} poly(A) tail and a 5{${'}$} m7G cap structure. We also show that HuD directly interacts with eIF4A. This interaction and the poly(A)-binding activity of HuD are critical for its translational enhancer function~because HuD-eIF4A- and HuD-poly(A)-binding mutants fail to stimulate translation. We show that translation of HCV IRES mRNA, which is eIF4A independent, is not stimulated by HuD. We also find that the eIF4A and poly(A)-binding activities of HuD are not only important for stimulating translation but also are essential for HuD-induced neurite outgrowth in PC12 cells. This example of cap-dependent translational regulation might explain at least in part how HuD triggers the induction of neuronal differentiation.},
  keywords = {MOLNEURO,RNA}
}

