  author = {Park, Ji-Hong and Nguyen, Tram Thi Ngoc and Lee, Eun-Mi and {Castro-Aceituno}, Veronica and Wagle, Ram and Lee, Kwang-Soon and Choi, Juyoung and Song, Young-Han},
  year = {2019},
